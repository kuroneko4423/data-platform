<?xml version="1.0"?>
<clickhouse>
    <users>
        <!-- デフォルトユーザー -->
        <default>
            <password>clickhouse123</password>
            <networks>
                <ip>::/0</ip>
            </networks>
            <profile>default</profile>
            <quota>default</quota>
            <access_management>1</access_management>
        </default>
        
        <!-- 分析用ユーザー -->
        <analyst>
            <password>analyst123</password>
            <networks>
                <ip>::/0</ip>
            </networks>
            <profile>default</profile>
            <quota>default</quota>
        </analyst>
        
        <!-- ETL用ユーザー -->
        <etl_user>
            <password>etl123</password>
            <networks>
                <ip>::/0</ip>
            </networks>
            <profile>default</profile>
            <quota>default</quota>
        </etl_user>
        
        <!-- 読み取り専用ユーザー -->
        <readonly_user>
            <password>readonly123</password>
            <networks>
                <ip>::/0</ip>
            </networks>
            <profile>readonly</profile>
            <quota>default</quota>
        </readonly_user>
    </users>
    
    <profiles>
        <default>
            <max_memory_usage>10000000000</max_memory_usage>
            <max_execution_time>300</max_execution_time>
            <load_balancing>random</load_balancing>
        </default>
        <readonly>
            <readonly>1</readonly>
            <max_memory_usage>5000000000</max_memory_usage>
            <max_execution_time>120</max_execution_time>
        </readonly>
    </profiles>
    
    <quotas>
        <default>
            <interval>
                <duration>3600</duration>
                <queries>1000</queries>
                <errors>100</errors>
                <result_rows>1000000000</result_rows>
                <read_rows>10000000000</read_rows>
                <execution_time>3600</execution_time>
            </interval>
        </default>
    </quotas>
</clickhouse>
